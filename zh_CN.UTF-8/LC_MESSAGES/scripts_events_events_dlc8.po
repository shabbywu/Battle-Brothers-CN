msgid "Glad he fights with "
msgstr ""

# scripts/events/events/dlc8/shadow_dissection_event.nut
msgid "May they watch over us."
msgstr ""

# scripts/events/events/dlc8/shadow_dissection_event.nut
msgid "Glad he fights alongside "
msgstr ""

# scripts/events/events/dlc8/shadow_dissection_event.nut
msgid "It sorta looked like a worm to me."
msgstr ""

# scripts/events/events/dlc8/shadow_dissection_event.nut
msgid "Strange shadows for a strange night."
